{
  "name": "veterinary-medical-records-docs-tooling",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "docs:lint": "markdownlint-cli2 \"docs/README.md\" \"docs/projects/veterinary-medical-records/**/*.md\" \"docs/shared/**/*.md\"",
    "docs:lint:fix": "markdownlint-cli2 --fix \"docs/README.md\" \"docs/projects/veterinary-medical-records/**/*.md\" \"docs/shared/**/*.md\"",
    "docs:links": "node scripts/check_docs_links.mjs",
    "docs:toc": "doctoc --github docs/README.md docs/project docs/shared",
    "docs:format": "prettier --write \"docs/README.md\" \"docs/projects/veterinary-medical-records/**/*.md\" \"docs/shared/**/*.md\"",
    "docs:format:check": "prettier --check \"docs/README.md\" \"docs/projects/veterinary-medical-records/**/*.md\" \"docs/shared/**/*.md\""
  },
  "devDependencies": {
    "doctoc": "^2.2.1",
    "markdown-link-check": "^3.13.7",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.8.1"
  }
}
